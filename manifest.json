{
  "name": "Scrappy",
  "description": "Scrape Emails from Webpage",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting", "declarativeNetRequest"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "icons":{
    "16":"icons/favicon-16x16.png",
    "32":"icons/favicon-32x32.png"
  }
}
